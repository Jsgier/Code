#include "msp.h"


/**
 * main.c
 */
void main(void)
{
    //Setup
	WDT_A->CTL = WDT_A_CTL_PW | WDT_A_CTL_HOLD;		     // stop watchdog timer

	//enable Timer A interrupt, set to count up, and use SMCLK
	TIMER_A0->CTL = TIMER_A_CTL_IE | TIMER_A_CTL_MC_1 | TIMER_A_CTL_TASSEL_2


	while(1)                                             // infinite loop
}
